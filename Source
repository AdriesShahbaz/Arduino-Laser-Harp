int yellow=8;
int black=9;
int orange=10;
int brown=11;
int laser = 12;

void setup() {
  pinMode(yellow,OUTPUT);
  pinMode(black,OUTPUT);
  pinMode(orange,OUTPUT);
  pinMode(brown,OUTPUT);
  pinMode(laser, OUTPUT);
}

void loop() {
  forward();
}

void forward(){
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(brown,HIGH);
  digitalWrite(laser, LOW);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(laser, LOW);
  digitalWrite(brown,LOW);
  digitalWrite(orange,HIGH);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(laser, LOW);
  digitalWrite(orange,LOW);
  digitalWrite(black,HIGH);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(laser, LOW);
  digitalWrite(black,LOW);
  digitalWrite(yellow,HIGH);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(laser, LOW);
  digitalWrite(yellow,LOW);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(laser, LOW);
  for(int i = 0; i < 2; i++) {
    digitalWrite(laser, HIGH);
    delay(100);
    digitalWrite(yellow, HIGH);
    digitalWrite(laser, LOW);
    delay(30);
    digitalWrite(laser, HIGH);
    delay(100);
    digitalWrite(laser, LOW);
    digitalWrite(yellow, LOW);
    digitalWrite(black, HIGH);
    delay(30);
    digitalWrite(laser, HIGH);
    delay(100);
    digitalWrite(laser, LOW);
    digitalWrite(black, LOW);
    digitalWrite(orange, HIGH);
    delay(30);
    digitalWrite(laser, HIGH);
    delay(100);
    digitalWrite(laser, LOW);
    digitalWrite(orange, LOW);
    digitalWrite(brown, HIGH);
    delay(30);
    digitalWrite(laser, HIGH);
    delay(100);
    digitalWrite(laser, LOW);
    digitalWrite(brown, LOW);
    delay(30);
    digitalWrite(laser, HIGH);
    delay(100);
    digitalWrite(laser, LOW);
  }
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(brown,HIGH);
  digitalWrite(laser, LOW);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(brown,LOW);
  digitalWrite(orange,HIGH);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(laser, LOW);
  digitalWrite(orange,LOW);
  digitalWrite(black,HIGH);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(laser, LOW);
  digitalWrite(black,LOW);
  digitalWrite(yellow,HIGH);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(yellow,LOW);
  delay(30);
  digitalWrite(laser, HIGH);
  delay(100);
  digitalWrite(laser, LOW);
}
